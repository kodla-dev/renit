{
  "name": "renit",
  "version": "0.0.0",
  "description": "The small framework with powerful features",
  "homepage": "https://renit.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/kodla-dev/renit",
    "directory": "packages/renit"
  },
  "license": "MIT",
  "author": {
    "name": "Kodla",
    "email": "dev@kodla.net"
  },
  "type": "module",
  "bin": {
    "renit": "./src/core/interface/index.js"
  },
  "exports": {
    ".": {
      "default": "./src/index.js",
      "types": "./type/index.d.ts"
    },
    "./core": {
      "default": "./src/core/index.js",
      "types": "./type/index.d.ts"
    },
    "./env": {
      "default": "./src/core/interface/env.js",
      "types": "./type/index.d.ts"
    },
    "./config": {
      "default": "./src/core/interface/config.js",
      "types": "./type/index.d.ts"
    },
    "./runtime": {
      "default": "./src/core/runtime/index.js",
      "types": "./type/index.d.ts"
    },
    "./helper": {
      "default": "./src/helpers/index.js",
      "types": "./type/index.d.ts"
    },
    "./collect": {
      "default": "./src/libraries/collect/index.js",
      "types": "./type/index.d.ts"
    },
    "./event": {
      "default": "./src/libraries/event/index.js",
      "types": "./type/index.d.ts"
    },
    "./is": {
      "default": "./src/libraries/is/index.js",
      "types": "./type/index.d.ts"
    },
    "./math": {
      "default": "./src/libraries/math/index.js",
      "types": "./type/index.d.ts"
    },
    "./router": {
      "default": "./src/libraries/router/index.js",
      "types": "./type/index.d.ts"
    },
    "./server": {
      "default": "./src/libraries/server/index.js",
      "types": "./type/index.d.ts"
    },
    "./store": {
      "default": "./src/libraries/store/index.js",
      "types": "./type/index.d.ts"
    },
    "./to": {
      "default": "./src/libraries/to/index.js",
      "types": "./type/index.d.ts"
    }
  },
  "main": "./src/index.js",
  "module": "./src/index.js",
  "types": "./type/index.d.ts",
  "files": [
    "src",
    "type",
    "./*.d.ts"
  ],
  "scripts": {
    "prebuild": "rimraf type *.d.ts -g",
    "build": "node build"
  },
  "dependencies": {
    "astring": "^1.8.6",
    "lightningcss": "^1.26.0",
    "meriyah": "^4.5.0",
    "vite": "^5.4.0"
  },
  "engines": {
    "node": ">= 18"
  }
}
